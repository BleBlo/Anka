{
  "id": "filter_006",
  "category": "filter",
  "description": "Filter transactions where amount is not equal to 0",
  "input_schema": {
    "transactions": [
      {"name": "txn_id", "type": "INT"},
      {"name": "description", "type": "STRING"},
      {"name": "amount", "type": "DECIMAL"}
    ]
  },
  "test_cases": [
    {
      "input": {
        "transactions": [
          {"txn_id": 1, "description": "Payment", "amount": 100.0},
          {"txn_id": 2, "description": "Void", "amount": 0},
          {"txn_id": 3, "description": "Refund", "amount": -50.0}
        ]
      },
      "expected_output": [
        {"txn_id": 1, "description": "Payment", "amount": 100.0},
        {"txn_id": 3, "description": "Refund", "amount": -50.0}
      ]
    },
    {
      "input": {
        "transactions": [
          {"txn_id": 1, "description": "A", "amount": 0},
          {"txn_id": 2, "description": "B", "amount": 0}
        ]
      },
      "expected_output": []
    },
    {
      "input": {
        "transactions": [
          {"txn_id": 1, "description": "A", "amount": 1.0}
        ]
      },
      "expected_output": [
        {"txn_id": 1, "description": "A", "amount": 1.0}
      ]
    }
  ]
}
