{
  "id": "pipeline_008",
  "category": "pipeline",
  "description": "Sort products by price ascending, limit to first 5, select name and price",
  "input_schema": {
    "products": [
      {"name": "id", "type": "INT"},
      {"name": "name", "type": "STRING"},
      {"name": "price", "type": "DECIMAL"},
      {"name": "stock", "type": "INT"}
    ]
  },
  "test_cases": [
    {
      "input": {
        "products": [
          {"id": 1, "name": "A", "price": 50.0, "stock": 10},
          {"id": 2, "name": "B", "price": 20.0, "stock": 20},
          {"id": 3, "name": "C", "price": 30.0, "stock": 15},
          {"id": 4, "name": "D", "price": 10.0, "stock": 5},
          {"id": 5, "name": "E", "price": 40.0, "stock": 25},
          {"id": 6, "name": "F", "price": 25.0, "stock": 30}
        ]
      },
      "expected_output": [
        {"name": "D", "price": 10.0},
        {"name": "B", "price": 20.0},
        {"name": "F", "price": 25.0},
        {"name": "C", "price": 30.0},
        {"name": "E", "price": 40.0}
      ]
    },
    {
      "input": {
        "products": [
          {"id": 1, "name": "X", "price": 100.0, "stock": 1}
        ]
      },
      "expected_output": [
        {"name": "X", "price": 100.0}
      ]
    },
    {
      "input": {
        "products": []
      },
      "expected_output": []
    }
  ]
}
