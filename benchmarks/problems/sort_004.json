{
  "id": "sort_004",
  "category": "sort",
  "description": "Sort orders by quantity in ascending order",
  "input_schema": {
    "orders": [
      {"name": "order_id", "type": "INT"},
      {"name": "product", "type": "STRING"},
      {"name": "quantity", "type": "INT"}
    ]
  },
  "test_cases": [
    {
      "input": {
        "orders": [
          {"order_id": 1, "product": "A", "quantity": 10},
          {"order_id": 2, "product": "B", "quantity": 5},
          {"order_id": 3, "product": "C", "quantity": 15}
        ]
      },
      "expected_output": [
        {"order_id": 2, "product": "B", "quantity": 5},
        {"order_id": 1, "product": "A", "quantity": 10},
        {"order_id": 3, "product": "C", "quantity": 15}
      ]
    },
    {
      "input": {
        "orders": [
          {"order_id": 1, "product": "X", "quantity": 1}
        ]
      },
      "expected_output": [
        {"order_id": 1, "product": "X", "quantity": 1}
      ]
    },
    {
      "input": {
        "orders": [
          {"order_id": 1, "product": "A", "quantity": 3},
          {"order_id": 2, "product": "B", "quantity": 1},
          {"order_id": 3, "product": "C", "quantity": 2}
        ]
      },
      "expected_output": [
        {"order_id": 2, "product": "B", "quantity": 1},
        {"order_id": 3, "product": "C", "quantity": 2},
        {"order_id": 1, "product": "A", "quantity": 3}
      ]
    }
  ]
}
