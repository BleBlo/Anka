{
  "id": "str_003",
  "category": "strings",
  "difficulty": "easy",
  "description": "Trim whitespace",
  "prompt": "Add a column 'name_clean' that removes leading and trailing whitespace from the name column",
  "input_schema": {
    "users": {
      "columns": ["id", "name"],
      "types": ["INT", "STRING"]
    }
  },
  "test_cases": [
    {
      "input": {
        "users": [
          {"id": 1, "name": "  Alice  "},
          {"id": 2, "name": "Bob"},
          {"id": 3, "name": "\t Charlie \n"}
        ]
      },
      "expected": [
        {"id": 1, "name": "  Alice  ", "name_clean": "Alice"},
        {"id": 2, "name": "Bob", "name_clean": "Bob"},
        {"id": 3, "name": "\t Charlie \n", "name_clean": "Charlie"}
      ]
    }
  ]
}
