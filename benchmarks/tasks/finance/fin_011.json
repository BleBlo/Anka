      "expected": [
        {"ticker": "GOOGL", "total_pnl": 1200.0, "trade_count": 2, "avg_pnl": 600.0},
        {"ticker": "AAPL", "total_pnl": 600.0, "trade_count": 3, "avg_pnl": 200.0},
        {"ticker": "MSFT", "total_pnl": -100.0, "trade_count": 1, "avg_pnl": -100.0}
      ]
